generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model product {
  id          String   @id
  description String
  price       Float
  quantity    Int
  category    String   @default("")
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
